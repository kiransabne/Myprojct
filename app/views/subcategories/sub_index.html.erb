<p id="notice"><%= notice %></p>
<b>
<%= link_to "Go to category", root_path%></b>
<h2>SubCategories</h2>
  <table>

  <tbody>
    <% @subcategories.each do |sub| %>
      <ul >
        <li class = "list">
          <%=sub.name %>
        </li><br>
        <li>
          <%= link_to image_tag(sub.image_url), indexx_products_path(:sub => sub.id) %>
        </li>
      </ul>    
      <% end %>
  </tbody>
</table>